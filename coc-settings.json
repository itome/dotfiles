{
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.errorSign": "◉",
  "diagnostic.warningSign": "◉",
  "diagnostic.infoSign": "◉",
  "diagnostic.hintSign": "◉",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },
    "rust": {
      "command": "ra_lsp_server",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  },
  "css.validate": false
}
